{
  "plugins": [
    "remark-preset-lint-recommended",
    "remark-preset-lint-consistent",
    "remark-preset-lint-markdown-style-guide",
    "remark-frontmatter",
    "remark-math",
    "remark-footnotes",
    ["remark-lint-file-extension", false],
    ["remark-lint-no-file-name-irregular-characters", false],
    ["remark-lint-maximum-line-length", 500],
    ["remark-lint-no-shortcut-reference-link", false],
    ["remark-lint-no-undefined-references", false],
    ["remark-lint-emphasis-marker", false],
    ["remark-lint-fenced-code-flag", false],
    ["remark-lint-no-duplicate-headings", false],
    ["remark-lint-maximum-heading-length", false],
    ["remark-lint-no-multiple-toplevel-headings", false],
    ["remark-lint-list-item-indent", "space"],
    ["remark-lint-list-item-bullet-indent", false],
    ["remark-lint-unordered-list-marker-style", false],
    ["remark-lint-list-item-spacing", false],
    ["remark-lint-list-item-content-indent", false],
    ["remark-lint-code-block-style", false],
    ["remark-lint-no-inline-padding", false],
    ["remark-lint-no-consecutive-blank-lines", false],
    ["remark-lint-strong-marker", false],
    ["remark-lint-table-pipe-alignment", false],
    ["remark-lint-table-cell-padding", false],
    ["remark-lint-table-pipes", false],
    ["remark-lint-no-literal-urls", false]
  ]
}
